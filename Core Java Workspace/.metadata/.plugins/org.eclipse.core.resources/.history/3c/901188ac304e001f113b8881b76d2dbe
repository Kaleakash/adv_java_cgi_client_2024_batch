package finalkeyword;



class Bike {
	void speed() {
		System.out.println("60km/hr");
	}
}
class Honda extends Bike {		
	void speed() {
		System.out.println("60km/hr");
	}
	void colour() {
		System.out.println("Red Colour");
	}
}
public class FinalExample {

	public static void main(String[] args) {
		final int A=10;
		//A=20;

	}

}
