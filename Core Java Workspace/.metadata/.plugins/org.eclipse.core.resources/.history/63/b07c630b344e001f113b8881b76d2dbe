package statickeyword;
class Info {
	int a;	// non static 
	static int b;
	void dis() {
		System.out.println("a "+a);
		System.out.println("b "+b);
	}
}
public class StaticExample1 {
	public static void main(String[] args) {
		Info i1 = new Info();		// heap memory 
		Info i2 = new Info();		// heap memory 

	}

}
